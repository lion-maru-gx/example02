<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jp.gr.java_conf.lion_maru_gx.example.example02.Page1Controller">
   <center>
      <AnchorPane prefHeight="400.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
           <GridPane AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="100.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label fx:id="voiceName01" text="Label" />
                  <Label fx:id="voiceName02" text="Label" GridPane.rowIndex="1" />
                  <Label fx:id="voiceName03" text="Label" GridPane.rowIndex="2" />
                  <Label fx:id="voiceName04" text="Label" GridPane.rowIndex="3" />
                  <Label fx:id="voiceName05" text="Label" GridPane.rowIndex="4" />
                  <Label fx:id="voiceName06" text="Label" GridPane.rowIndex="5" />
                  <Label fx:id="voiceName07" text="Label" GridPane.rowIndex="6" />
                  <Label fx:id="voiceName08" text="Label" GridPane.rowIndex="7" />
                  <Label fx:id="voiceName09" text="Label" GridPane.columnIndex="1" />
                  <Label fx:id="voiceName10" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="voiceName11" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="voiceName12" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label fx:id="voiceName13" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label fx:id="voiceName14" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <Label fx:id="voiceName15" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                  <Label fx:id="voiceName16" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="7" />
               </children>
            </GridPane>
            <ChoiceBox fx:id="bankChoiceBox" layoutX="100.0" layoutY="35.0" prefWidth="150.0" />
         </children>
      </AnchorPane>
   </center>
   <top>
      <HBox spacing="10.0" BorderPane.alignment="CENTER_LEFT">
         <children>
            <Button mnemonicParsing="false" onAction="#handleButtonAction" text="戻る" />
            <Button mnemonicParsing="false" onAction="#onClickRestoreBtn" text="読込み" />
            <Button mnemonicParsing="false" onAction="#onClickSaveBtn" text="保存" />
            <Button alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" text="送信" />
         </children>
         <BorderPane.margin>
            <Insets bottom="15.0" left="12.0" right="12.0" top="15.0" />
         </BorderPane.margin>
      </HBox>
   </top>
</BorderPane>
